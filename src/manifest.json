{
    "manifest_version": 3,
    "name": "LichessBulletBlock",
    "version": "1.2.0",
    "description": "Remove Bullet Games from Lichess, optionally block Blitz Games and Puzzle Variants",
    "key": "",
    "content_security_policy": {
        "default-src": "self",
        "frame-ancestors": "none"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "/icons/16.png",
        "48": "/icons/48.png",
        "128": "/icons/128.png"
    },
    "action": {
        "default_icon": {
            "16": "/images/16.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
        }
    },
    "content_scripts": [
        {
            "js": [
                "content.js",
                "puzzle.js"
            ],
            "matches": [
                "https://lichess.org/*"
            ]
        },
        {
            "js": [
                "quotes.js"
            ],
            "matches": [
                "https://lichess.org/"
            ]
        }
    ]
}